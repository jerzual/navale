{
  "name": "navale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint --fix --extension=ts,tsx",
    "pretest": "npm run tslint",
    "start": "vite",
    "test": "vitest",
    "test-only": "vitest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc && vite build",
    "deploy": "npm run clean && npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@babylonjs/core": "^5.13.3",
    "@ngneat/react-rxjs": "^1.1.0",
    "@react-three/fiber": "^8.0.27",
    "nanoid": "^4.0.0",
    "picnic": "^7.1.0",
    "react": "^18.2.0",
    "react-babylonjs": "^3.1.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "rxjs": "^7.5.5",
    "seedrandom": "^3.0.5",
    "simplex-noise": "^3.0.1",
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "~4.6.0",
    "vite": "^2.9.13",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^0.16.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*": "prettier -w -u"
  }
}
